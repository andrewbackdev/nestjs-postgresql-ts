{
	"name": "nestjs-postgresql-ts",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"prestart": "npm run db:up",
		"start": "cross-env ENV_FILE=.env nest start --watch",
		"start:debug": "cross-env ENV_FILE=.env nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "cross-env ENV_FILE=ci.env jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "cross-env ENV_FILE=ci.env node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"pretest:e2e": "npm run ci-db:up",
		"test:e2e": "cross-env ENV_FILE=ci.env jest --config ./test/jest-e2e.config.ts || true",
		"posttest:e2e": "npm run ci-db:down",
		"migration:create": "npm run build && cross-env ENV_FILE=.env node dist/app/databases/postgres/migration/create-migration.js",
		"premigration:generate": "npm run migration-db:up",
		"migration:generate": "npm run build && cross-env ENV_FILE=migration.env typeorm migration:generate ./src/migrations/post-refactoring -d dist/app/databases/postgres/data-source.js",
		"premigration:run": "npm run db:up && npm run migration-db:up",
		"migration:run": "npm run build && npm run migration-run:local && npm run migration-run:migration",
		"migration-run:local": "cross-env ENV_FILE=.env typeorm migration:run -d dist/app/databases/postgres/data-source.js",
		"migration-run:migration": "cross-env ENV_FILE=migration.env typeorm migration:run -d dist/app/databases/postgres/data-source.js",
		"db:up": "docker-compose -f docker-compose.yml up -d db",
		"db:down": "docker-compose -f docker-compose.yml stop db",
		"migration-db:up": "docker-compose -f docker-compose.yml up -d migration.db",
		"migration-db:down": "docker-compose -f docker-compose.yml stop migration.db",
		"ci-db:up": "docker-compose -f docker-compose.yml up -d ci.db",
		"ci-db:down": "docker-compose -f docker-compose.yml down --volumes --remove-orphans ci.db"
	},
	"dependencies": {
		"@fastify/helmet": "^10.1.1",
		"@fastify/static": "^6.10.1",
		"@nestjs/common": "^9.0.0",
		"@nestjs/core": "^9.0.0",
		"@nestjs/jwt": "^10.0.3",
		"@nestjs/platform-fastify": "^9.4.0",
		"@nestjs/swagger": "^6.3.0",
		"@nestjs/throttler": "^4.0.0",
		"@nestjs/typeorm": "^9.0.1",
		"bcrypt": "^5.1.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"lodash": "^4.17.21",
		"pg": "^8.11.3",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0",
		"tsconfig-paths": "^4.2.0",
		"typeorm": "^0.3.16",
		"winston": "^3.10.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.0.0",
		"@nestjs/schematics": "^9.0.0",
		"@nestjs/testing": "^9.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/jest": "29.5.0",
		"@types/lodash": "^4.14.194",
		"@types/node": "18.15.11",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "29.5.0",
		"jest-extended": "^3.2.4",
		"prettier": "^2.3.2",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "29.0.5",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"typescript": "^4.7.4"
	},
	"engines": {
		"node": ">=18.16.0 <19.0.0",
		"npm": ">=9.6.0 <10"
	}
}
